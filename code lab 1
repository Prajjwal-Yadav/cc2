//temperture conversion
#include<stdio.h>

void main()
{
    int a,b;
    printf("Enter the temperature to be converted\n");
    scanf("%d",&a);
    b=((a-32)*5)/9;
    printf("Converted temperature\n");
    printf("%d",b);
    
}

//greatest of two number
       #include<stdio.h>
       
       void main()
       {

              int n1,n2;
              
              printf("\nEnter 1st number : "); 
              scanf("%d",&n1);

              printf("\nEnter 2nd number : "); 
              scanf("%d",&n2);
              
              if(n1 > n2)              
                     printf("\n1st number is greatest.");
              else
                     printf("\n2nd number is greatest.");

       }
