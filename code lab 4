//even number between
#include<stdio.h>
void main()
{
    int m,n;
    printf("enter\n");
    scanf("%d%d",&m,&n);
    for(int i=m;i<=n;i++)
    {
        if(i%2==0)
        printf("%d ",i);
    }
}

//multiple of a number
#include<stdio.h>
void main()
{
    int n;
    printf("enter\n");
    scanf("%d",&n);
    if(n!=0){
        for(int i=n;i<=100;i++){
            if(i%n==0)
            printf("%d  ",i);
        }
    }
    else
        printf("0");
}

//sum of digit
#include<stdio.h>
void main(){
    int n,s=0,i;
    printf("enter\n");
    scanf("%d",&n);
    while(n>0)
    {
        i=n%10;
        s=s+i;
        n=n/10;
    }
    printf("sum=%d",s);
    
}
